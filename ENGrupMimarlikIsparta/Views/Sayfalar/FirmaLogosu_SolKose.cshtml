@using ENGrupMimarlikIsparta.Models.Siniflar
@model List<IletisimBilgileri>

@foreach (var item in Model)
{
    if (!string.IsNullOrEmpty(item.FirmaLogosu))
    {
        <img class="logo-img" style="width:60px;height:60px; text-decoration:none"
             src="@item.FirmaLogosu" alt="~/Image/bina1.jpg">
    }
    if (string.IsNullOrEmpty(item.FirmaLogosu))
    {
        <img class="logo-img" style="width:60px;height:60px; text-decoration:none"
             src="~/ENMimarlik/img/logo.png" alt="~/Image/bina1.jpg">
    }

}





